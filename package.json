{
  "name": "webframework",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "start": "concurrently \"npm --prefix apps/back start\" \"npm --prefix apps/front run dev\"",
    "build": "npm --prefix apps/front run build",
    "lint": "npm --prefix apps/front run lint"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "^16.1.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "react-datepicker": "^9.1.0"
  }
}
